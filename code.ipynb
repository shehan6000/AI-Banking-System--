def authenticate(user, pwd):
    if user not in st.session_state.users:
        return False
    return st.session_state.users[user]["password"] == hash_password(pwd)

def add_transaction(user, txn_type, amount, desc):
    st.session_state.transactions.append({
        "username": user,
        "type": txn_type,
        "amount": amount,
        "description": desc,
        "timestamp": datetime.now().strftime("%Y-%m-%d %H:%M:%S"),
        "balance": st.session_state.users[user]["balance"]
    })

def detect_fraud(df):
    if len(df) < 10:
        return []
    features = df[["amount"]].values.reshape(-1, 1)
    iso = IsolationForest(contamination=0.1, random_state=42)
    predictions = iso.fit_predict(features)
    return df[predictions == -1].index.tolist()

