# Helper functions
def hash_password(pwd):
    return hashlib.sha256(pwd.encode()).hexdigest()

def create_account(user, pwd, balance=1000):
    if user in st.session_state.users:
        return False
    st.session_state.users[user] = {
        "password": hash_password(pwd),
        "balance": balance,
